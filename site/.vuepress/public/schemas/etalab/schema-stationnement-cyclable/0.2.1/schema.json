   "fields": [
      {
         "name": "id_local",
         "description": "Identifiant unique du stationnement dans les bases de données de la collectivité territoriale productrice de données",
         "example": "STSI01",
         "type": "string",
         "constraints": {
            "required": true
         }
      },
      {
         "name": "id_osm",
         "description": "Identifiant OpenStreetMap, si les données proviennent d'OSM. On compose l'identifiant en utilisant la clef \"n\" s'il s'agit d'un noeud, \"w\" s'il s'agit d'un chemin et \"r\" s'il s'agit d'une relation.",
         "example": "n34172",
         "type": "string",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "code_com",
         "description": "Code INSEE de la commune où se trouve le stationnement vélo",
         "example": "n71729735",
         "type": "string",
         "constraints": {
            "required": false,
            "pattern": "^([013-9]\\d|2[AB1-9])\\d{3}$"
         }
      },
      {
         "name": "xlong",
         "description": "La longitude en degrés décimaux (point comme séparateur décimal, avec au moins 4 chiffres après le point décimal) de la localisation du centre de l'emplacement de stationnement exprimée dans le système de coordonnées WGS84.",
         "example": "1.452323",
         "type": "number",
         "constraints": {
            "required": true,
            "minimum": -180,
            "maximum": 180
         }
      },
      {
         "name": "ylat",
         "description": "La latitude en degrés décimaux (point comme séparateur décimal, avec au moins 4 chiffres après le point décimal) de la localisation de centre de l'emplacement de stationnement exprimée dans le système de coordonnées WGS84.",
         "example": "46.59698",
         "type": "number",
         "constraints": {
            "required": true,
            "minimum": -90,
            "maximum": 90
         }
      },
      {
         "name": "capacite",
         "description": "Nombre de places de stationnement disponibles sur l'emplacement y compris les places adaptées à des vélos spéciaux (comme les vélos de grande taille).",
         "example": "14",
         "type": "integer",
         "constraints": {
            "required": true
         }
      },
      {
         "name": "capacite_cargo",
         "description": "Nombre de places de stationnement disponibles pour les vélos de grande taille",
         "example": "2",
         "type": "integer",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "type_accroche",
         "description": "Mode d'accrochage possible du vélo aux équipements de stationnement",
         "example": "CADRE ET ROUE",
         "type": "string",
         "constraints": {
            "required": true,
            "enum": [
               "ROUE",
               "CADRE",
               "CADRE ET ROUE",
               "SANS ACCROCHE"
            ]
         }
      },
      {
         "name": "mobilier",
         "description": "Type de mobilier de stationnement",
         "example": "ARCEAU",
         "type": "string",
         "constraints": {
            "required": false,
            "enum": [
               "ARCEAU",
               "RATELIER",
               "RACK DOUBLE ETAGE",
               "CROCHET",
               "SUPPORT GUIDON",
               "POTELET",
               "ARCEAU VELO GRANDE TAILLE",
               "AUCUN EQUIPEMENT",               
               "AUTRE"
            ]
         }
      },
      {
         "name": "acces",
         "description": "Mode d'accès à l'emplacement de stationnement",
         "example": "LIBRE ACCES",
         "type": "string",
         "constraints": {
            "required": false,
            "enum": [
               "LIBRE ACCES",
               "ABONNEMENT OU INSCRIPTION PREALABLE",
               "PRIVE"
            ]
         }
      },
      {
         "name": "gratuit",
         "description": "L'usage de l'équipement de stationnement est-il gratuit ?",
         "example": "true",
         "type": "boolean",
         "trueValues": [
            "true",
            "True",
            "TRUE",
            "1",
            "vrai",
            "Vrai",
            "VRAI"
         ],
         "falseValues": [
            "false",
            "False",
            "FALSE",
            "0",
            "faux",
            "Faux",
            "FAUX"
         ],
         "constraints": {
            "required": false
         }
      },
      {
         "name": "protection",
         "description": "Protection du stationnement",
         "example": "STATIONNEMENT NON FERME",
         "type": "string",
         "constraints": {
            "required": false,
            "enum": [
               "STATIONNEMENT NON FERME",
               "CONSIGNE COLLECTIVE FERMEE",
               "BOX INDIVIDUEL FERME",
               "AUTRE"
            ]
         }
      },
      {
         "name": "couverture",
         "description": "Le stationnement est-il couvert et est donc protégé de la pluie, de la neige et de la grêle ?",
         "example": "true",
         "type": "boolean",
         "trueValues": [
            "true",
            "True",
            "TRUE",
            "1",
            "vrai",
            "Vrai",
            "VRAI"
         ],
         "falseValues": [
            "false",
            "False",
            "FALSE",
            "0",
            "faux",
            "Faux",
            "FAUX"
         ],
         "constraints": {
            "required": false
         }
      },
      {
         "name": "surveillance",
         "description": "Le stationnement est-il surveillé par un gardien ou par de la vidéosurveillance ?",
         "example": "true",
         "type": "boolean",
         "trueValues": [
            "true",
            "True",
            "TRUE",
            "1",
            "vrai",
            "Vrai",
            "VRAI"
         ],
         "falseValues": [
            "false",
            "False",
            "FALSE",
            "0",
            "faux",
            "Faux",
            "FAUX"
         ],
         "constraints": {
            "required": false
         }
      },
      {
         "name": "lumiere",
         "description": "L'emplacement est-il éclairé la nuit (par de l'éclairage urbain ou dédié à l'emplacement) ?",
         "example": "true",
         "type": "boolean",
         "trueValues": [
            "true",
            "True",
            "TRUE",
            "1",
            "vrai",
            "Vrai",
            "VRAI"
         ],
         "falseValues": [
            "false",
            "False",
            "FALSE",
            "0",
            "faux",
            "Faux",
            "FAUX"
         ],
         "constraints": {
            "required": false
         }
      },
      {
         "name": "url_info",
         "description": "Site web d'information sur l'emplacement (abonnement/réservation...)",
         "example": "https://www.exemple.fr/stationnementvelo-maville/",
         "type": "string",
         "format": "uri",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "d_service",
         "description": "Date de mise en service du stationnement vélo (Notation ISO 8601, format AAAA)",
         "example": "2017",
         "type": "integer",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "source",
         "description": "Nom de l'organisation ayant fourni les données",
         "example": "Grand Poitiers",
         "type": "string",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "proprietaire",
         "description": "Nom de l'organisation propriétaire de l'emplacement de stationnement",
         "example": "Ville de Poitiers",
         "type": "string",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "gestionnaire",
         "description": "Nom de l'organisation gestionnaire de l'emplacement de stationnement",
         "example": "Ville de Poitiers",
         "type": "string",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "date_maj",
         "description": "Date de dernière mise à jour des données (Notation ISO 8601, format AAAA-MM-JJ)",
         "example": "2020-02-27",
         "type": "date",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "commentaires",
         "description": "Commentaires divers sur le stationnement vélo, conditions d'accès, précisions sur la localisation, tarifs...",
         "example": "L'accès à l'emplacement se fait en sous-sol de la gare",
         "type": "string",
         "constraints": {
            "required": false
         }
      }
   ],
   "primaryKey": "id_local"
}
